#%RAML 0.8
---
title: Example API
baseUri: http://localhost:3000
securedBy: [oauth_2_0]
securitySchemes:
  - oauth_1_0:
      type: OAuth 1.0
  - oauth_2_0:
      type: OAuth 2.0
/first:
  get:
    securedBy: [oauth_1_0]
  post:
    securedBy: [oauth_2_0]
/second:
  securedBy: [oauth_1_0]
  get:
    securedBy: [null, oauth_2_0]
  post:
    securedBy: [oauth_2_0]
